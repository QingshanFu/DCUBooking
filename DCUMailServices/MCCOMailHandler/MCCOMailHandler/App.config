<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <appSettings>
      <add key="LogFileName" value="MCCOMailHandler.log" /> <!-- 日志文件名，日志默认保存在应用程序同级目录，名称可以配置 -->
      <add key="DataBaseConnectionString" value="data source=.;initial catalog=MCCOData;User ID=sa;Password=1111" /> <!-- 数据库连接字符串 -->
      <add key="DataBaseReConnectTimes" value="5" /> <!-- 数据库重连次数 -->
      <add key="CCAlias" value="v-lufli" /> <!-- 邮件抄送者列表，多人用 ";" 间隔 -->
      <add key="MailTemplate" value="D:\Center1Booking\MailTemplate" /> <!-- 邮件模板存放路径 -->
      <add key="WebHomeUrl" value="http://keenfupc" /> <!-- 邮件模板存放路径 -->
    </appSettings>
</configuration>